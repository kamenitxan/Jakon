<div class="select_wrapper">
    <input id="js_foreign_{{ fieldHash }}" class="hidden js_foreign" placeholder="Search...">
    <select class="form-control" name="{{ field.name }}" id="{{ fieldHash }}" {% if field.disabled %}disabled{% endif %}>
    </select>
    <script>
        const ForeignObjectSelector = require("forms/j_foreign_object.js");
        new ForeignObjectSelector("{{ field.objectName }}", "{{ fieldHash }}");
    </script>
</div>

